cc_test {
    name: "oob_oggpack_look",
    gtest: true,
    test_suites: ["device-tests"],

    srcs: ["test_floor0.cpp"],

    // compile_multilib: "64",

    static_libs: [
        "libvorbisidec",
        "liblog",
    ],

    shared_libs: [
        "libutils",
        "liblog",
    ],

    cflags: [
        "-Wall",
        "-Werror",
    ],

    // Assembly is only written for ARM and cannot interop with Thumb.
    arch: {
        arm: {
            instruction_set: "arm",
        },
    },

    sanitize: {
        integer_overflow: true,
        misc_undefined: ["bounds"],
        diag: {
            integer_overflow: true,
            undefined: true,
            misc_undefined: ["bounds"],
        },
    },
}
